  "repository": {
    "type": "git",
    "url": "git+https://github.com/geoastronaute/materia-angular-universal.git"
  },
  "scripts": {
    "build": "ng build",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "build:prod": "npm run clean && npm run build:client && npm run build:server && npm run postbuild",
    "build:server": "tsc -p ./tsconfig.server.json && ncp ./server/models ./dist/server/models",
    "build:client": "ng build --prod",
    "postbuild": "node scripts/postbuild.js",
    "clean": "rimraf dist"
  },
